# Apache 2.2
<IfModule !mod_authz_core.c>
    Order deny,allow
    Deny from all

    <FilesMatch "(logo\.png|views/img/registration/+\.png)">
        Allow from All
    </FilesMatch>
</IfModule>

# Apache 2.4
<IfModule mod_authz_core.c>
    Require all denied

    <FilesMatch "(logo\.png|views/img/registration/+\.png)">
        Require all granted
    </FilesMatch>
</IfModule>
